@model List<MVCLab2.Models.Message>
@{
    ViewBag.Title = "Messages";
}

<p>
    @foreach (var m in Model)
    {
        @:<p>From: @m.User.UserName <br />
        @:Title: @m.Title <br />
        @:Date: @m.Date<br />
        @:Topic: @m.Topic <br />
        @: <h5>@m.Body</h5></p> <br />
    }
    <br />
</p>